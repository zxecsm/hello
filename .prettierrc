{
  "printWidth": 100,          
  "tabWidth": 2,              
  "useTabs": false,          
  "semi": true,               
  "singleQuote": true,        
  "trailingComma": "all",     
  "bracketSpacing": true,     
  "arrowParens": "always",    
  "htmlWhitespaceSensitivity": "css", 
  "endOfLine": "lf",          
  "overrides": [
    {
      "files": "*.less",
      "options": { "tabWidth": 2 } 
    },
    {
      "files": "*.html",
      "options": { "printWidth": 120 }  
    }
  ]
}
